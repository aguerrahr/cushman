@model wr_anit_cushman_one.Models.tsg039_colonias

@{
    ViewBag.Title = "Delete";
}

<div class="table-responsive container modal-content">
    <h4 class="well"><strong>¿Estas seguro de eliminar el registro?</strong></h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Municipio
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tsg038_municipios.nb_municipio)
        </dd>

        <dt>
            Estado
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cd_estado)
        </dd>

        <dt>
            Colonia
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nb_colonia)
        </dd>

        <dt>
            CP
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nu_cp)
        </dd>

        <dt>
            Fecha Modificación
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fh_modif)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        
        <div class="form-group container">
            <div class="col-md-offset-0 col-md-1">
                <input type="submit" value="Eliminar" class="btn btn-danger" />
            </div>
            <div class="col-md-offset-0 col-md-2">
                @Html.ActionLink("Regresar a la Lista", "Index", "Colonias", new { @class = "btn btn-danger btn-block" })
            </div>
        </div>
    }
</div>
