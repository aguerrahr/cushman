@model wr_anit_cushman_one.Models.tsg007_mercado

@{
    ViewBag.Title = "Details";
}

<div class="table-responsive container modal-content">
    <h4 class="well"><strong>Detalle Mercados</strong></h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Mercados
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nb_mercado)
        </dd>

        <dt>
            Fecha Modificación
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fh_modif)
        </dd>

    </dl>
    <hr />
    <div class="form-group container">
        <div class="col-md-offset-0 col-md-1">
            @Html.ActionLink("Editar", "Edit", "Mercados", new { id = Model.cd_mercado }, new { @class = "btn btn-danger btn-block" })
        </div>
        <div class="col-md-offset-0 col-md-2">
            @Html.ActionLink("Regresar a la Lista", "Index", "Mercados", new { @class = "btn btn-danger btn-block" })
        </div>
    </div>
    <br />
</div>
