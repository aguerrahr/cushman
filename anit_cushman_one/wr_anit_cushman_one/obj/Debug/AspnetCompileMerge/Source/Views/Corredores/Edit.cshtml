        <div class="form-group">   
            <div class="col-md-1">Corredor Industrial</div>         
            <div class="col-md-10">                 
                @Html.EditorFor(model => model.nb_corredor, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.nb_corredor, "", new { @class = "text-danger" })
            </div>
        </div>

        @*<div class="form-group invisible">
                <div class="col-md-1">Fecha Modificación</div>
                <div class="col-md-10">
                    @Html.EditorFor( model => model.fh_modif, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.fh_modif, "", new { @class = "text-danger" })
                </div>
            </div>*@

        <div class="form-group">
            <div class="col-md-offset-0 col-md-1">
                <input type="submit" value="Guardar" class="btn btn-danger" />
            </div>
            <div class="col-md-offset-0 col-md-2">
                @Html.ActionLink("Regresar a la Lista", "Index", "Corredores", new { @class = "btn btn-danger btn-block" })
            </div>
        </div>

        
     </div>
}


