@model wr_anit_cushman_one.Models.tsg008_corredor_ind

@{
    ViewBag.Title = "Edit";
}


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal table-responsive container modal-content">
        <h4 class="well"><strong>Editar Corredor Industrial</strong></h4>
        <hr />
