@model IEnumerable<wr_anit_cushman_one.Models.tsg002_nave_industrial>

@{
    ViewBag.Title = "ExportarExcel";
    var grdNavesInd = new WebGrid(source: Model, canPage:false);
}

<style type="text/css">
    table.gridtable {
        font-family: verdana, arial, sans-serif;
        font-size: 11px;
        color: #333333;
        border-width: 1px;
        border-color: #666666;
        border-collapse: collapse;
    }
    table.gridtable th {
        border-width: 1px;
        padding: 8px;
        border-style: solid;
        border-color: #666666;
        background-color: #dedede;
    }
    table.gridtable td {
        border-width: 1px;
        padding: 8px;
        border-style: solid;
        border-color: #666666;
        background-color: #ffffff;
    }
</style>

<h2>Naves Industriales</h2>
<div>
    Exportar Datos: @Html.ActionLink("Excel", "GetExcel", "ExportData")
</div>
<div>
    @grdNavesInd.GetHtml(
        tableStyle: "gridtable",
        columns: grdNavesInd.Columns(
                grdNavesInd.Column("cd_mercado", "Mercado"),
                grdNavesInd.Column("cd_corredor", "Corredor"),
                grdNavesInd.Column("nb_parque", "Paque"),
                grdNavesInd.Column("nb_nave", "Nombre Nave"),
                grdNavesInd.Column("nb_calle", "Calle"),
                grdNavesInd.Column("nu_direcion", "No."),
                grdNavesInd.Column("nu_cp", "CP"),
                grdNavesInd.Column("cd_estado", "Estado"),
                grdNavesInd.Column("cd_municipio", "Municipio"),
                grdNavesInd.Column("cd_colonia", "Colonia"),
                grdNavesInd.Column("nb_dir_nave", "Direccion")

            )
    )
</div>


