@model wr_anit_cushman_one.Models.tsg016_espesor

@{
    ViewBag.Title = "Details";
}

<div class="table-responsive container modal-content">
    <h4 class="well"><strong>Detalle Espesor</strong></h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Espesor
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nb_espesor)
        </dd>

        <dt>
            Fecha Modificación
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fh_modif)
        </dd>
    </dl>

        <div class="form-group container">
