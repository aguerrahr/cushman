@model wr_anit_cushman_one.Models.tsg024_cond_arr

@{
    ViewBag.Title = "Delete";
}

<div class="table-responsive container modal-content">
    <h4 class="well"><strong>¿Estas seguro de eliminar el registro?</strong></h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Condición Arrendamiento
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nb_cond_arr)
        </dd>

        <dt>
            Fecha Modificación
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fh_modif)
        </dd>

    </dl>
    <hr />
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-group container">
            <div class="col-md-offset-0 col-md-1">
                <input type="submit" value="Eliminar" class="btn btn-danger" />
            </div>
            <div class="col-md-offset-0 col-md-2">
                @Html.ActionLink("Regresar a la Lista", "Index", "CondicionesArrenda", new { @class = "btn btn-danger btn-block" })
            </div>
        </div>
    }
</div>
