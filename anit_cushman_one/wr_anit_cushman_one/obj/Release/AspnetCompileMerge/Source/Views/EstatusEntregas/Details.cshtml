@model wr_anit_cushman_one.Models.tsg017_st_entrega

@{
    ViewBag.Title = "Details";
}

<div class="table-responsive  container modal-content">
    <h4 class="well"><strong>Detalle Estatus Entrega</strong></h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Estatus Entrega
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nb_st_entrega)
        </dd>

        <dt>
            Fecha Modificación
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fh_modif)
        </dd>

    </dl>

    <div class="form-group container">
        <div class="col-md-offset-0 col-md-1">
            @Html.ActionLink("Editar", "Edit", "EstatusEntregas", new { id = Model.cd_st_entrega }, new { @class = "btn btn-danger btn-block" })
        </div>
        <div class="col-md-offset-0 col-md-2">
            @Html.ActionLink("Regresar a la Lista", "Index", "EstatusEntregas", new { @class = "btn btn-danger btn-block" })
        </div>
    </div>

</div>
