@model wr_anit_cushman_one.Models.tsg014_tp_lampara

@{
    ViewBag.Title = "Details";
}

<div class="table-responsive container modal-content">
    <h4 class="well"><strong>Detalle Tipo Lamparas</strong></h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Tipo Lamparas
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nb_tp_lampara)
        </dd>

        <dt>
            Fecha Modificación
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fh_modif)
        </dd>

    </dl>
    <hr />
    <div class="form-group container">
        <div class="col-md-offset-0 col-md-1">
            @Html.ActionLink("Editar", "Edit", "TiposLamparas", new { id = Model.cd_tp_lampara }, new { @class = "btn btn-danger btn-block" })
        </div>
        <div class="col-md-offset-0 col-md-2">
            @Html.ActionLink("Regresar a la Lista", "Index", "TiposLamparas", new { @class = "btn btn-danger btn-block" })
        </div>
    </div>
    <br />
</div>
